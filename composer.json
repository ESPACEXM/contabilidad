{
    "name": "proyecto/contabilidad",
    "type": "project",
    "description": "Sistema de GestiÃ³n Financiera y Contabilidad",
    "keywords": ["laravel", "contabilidad", "finanzas"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "livewire/livewire": "^3.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\Composer\\Scripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@fix-platform-check"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@fix-platform-check"
        ],
        "post-install-cmd": [
            "@fix-platform-check"
        ],
        "fix-platform-check": [
            "@php -r \"$file = 'vendor/composer/platform_check.php'; if (file_exists($file)) { $content = file_get_contents($file); $content = preg_replace('/PHP_VERSION_ID >= 80300/', 'PHP_VERSION_ID >= 80200', $content); $content = preg_replace('/>= 8\\.3\\.0/', '>= 8.2.0', $content); file_put_contents($file, $content); }\""
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.2.12"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
